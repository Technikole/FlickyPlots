{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/getQloo",
        "function": "getQloo"
      },
        {
    "source": "/getAiTourGuide",
    "function": "getAiTourGuide"
  }
    ]
  },
  "functions": {
    "source": "functions",
    "codebase": "default",
    "ignore": [
      "node_modules",
      ".git",
      "firebase-debug.log",
      "firebase-debug.*.log",
      "*.local"
    ]
  },
  "storage": {
    "rules": "storage.rules"
  }
}
